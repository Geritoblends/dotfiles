[global]
    font = "Inter", "JetBrainsMono NF" 12
    width = 300
    padding = 20
    corner_radius = 16
    frame_width = 4
    background = "#202020"  
    foreground = "#d3869b"
    frame_color = "#ebdbb2"
    gap_size = 5

[urgency_low]
    timeout = 3
    frame_color = "#ebdbb2"   
    foreground = "#d3869b"   

[urgency_normal]
    timeout = 5
    frame_color = "#ebdbb2" 

[urgency_critical]
    timeout = 15
    foreground = "#eb6f92"  
    frame_color = "#eb6f92"
